{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "JSON Schema for save",
	"type": "object",
	"properties": {
        "book_item_id": {
          "type": "integer",
          "description": "The identifier of the book item being lent."
        },
		    "user_id": {
          "type": "integer",
          "description": "The identifier of the user who reserved the book."
        }
      },
    "required": ["book_item_id", "user_id"],
	"additionalProperties": false
}
